{
  "name": "eventodd",
  "version": "1.0.4",
  "description": "EventOdd package is develop for practicing NPM function",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Poafs1/eventOdd.git"
  },
  "keywords": [
    "Number",
    "Event Odd"
  ],
  "author": "Poafs1",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Poafs1/eventOdd/issues"
  },
  "homepage": "https://github.com/Poafs1/eventOdd#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  },
  "files": [
    "lib/**/*"
  ]
}
